{
  "version": "0.2.0",
  "configurations": [
    


    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch first.js",
      "runtimeExecutable": "C:\\Program Files\\nodejs\\node.exe",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/first.js"
    }
  ]
}
